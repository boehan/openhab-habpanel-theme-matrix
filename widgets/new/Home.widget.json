{
    "template": "<div class=\"section top\">\n  <div class=\"row\">\n    <div class=\"col-xs-7 col-sm-4\">\n      <div class=\"title top\">{{itemValue('HABPanel_Dashboard')}}</div>\n    </div>\n    <div class=\"col-xs-5 col-sm-2\">\n      <div class=\"sectionIconContainer\">\n        <div class=\"control main\" ng-class=\"{true: 'on'}[itemValue('gLicht_Switch', true)=='ON']\" ng-click=\"sendCmd('gLicht_Switch', 'OFF')\"><svg viewBox=\"0 0 100 80\"><use xlink:href=\"/static/matrix-theme/material-icons.svg#lightbulb-outline\"></use></svg></div>\n        <div class=\"control main\" ng-class=\"{true: 'on'}[itemValue('gDevices_Switch', true)=='ON']\" ng-click=\"sendCmd('gDevices_Switch', 'OFF')\"><svg viewBox=\"0 0 100 80\"><use xlink:href=\"/static/matrix-theme/material-icons.svg#speaker-off\"></use></svg></div>\n      </div>\n    </div>\n    <div class=\"col-xs-12 col-sm-6\">\n      <div class=\"sectionIconContainer\">\n        <div class=\"control\" ng-class=\"{true: 'on'}[itemValue('HABPanel_Dashboard', true)=='System']\" ng-click=\"sendCmd('HABPanel_Dashboard', 'System')\"><svg viewBox=\"0 0 100 80\"><use xlink:href=\"/static/matrix-theme/material-icons.svg#memory\"></use></svg></div>\n        <div class=\"control\" ng-class=\"{true: 'on'}[itemValue('HABPanel_Dashboard', true)=='Heizung']\" ng-click=\"sendCmd('HABPanel_Dashboard', 'Heizung')\"><svg viewBox=\"0 0 100 80\"><use xlink:href=\"/static/matrix-theme/material-icons.svg#fire\"></use></svg></div>\n        <div class=\"control\" ng-class=\"{true: 'on'}[itemValue('HABPanel_Dashboard', true)=='Lueftung']\" ng-click=\"sendCmd('HABPanel_Dashboard', 'Lueftung')\"><svg viewBox=\"0 0 100 80\"><use xlink:href=\"/static/matrix-theme/material-icons.svg#fan\"></use></svg></div>\n        <div class=\"control\" ng-class=\"{true: 'on'}[itemValue('HABPanel_Dashboard', true)=='Musik']\" ng-click=\"sendCmd('HABPanel_Dashboard', 'Musik')\"><svg viewBox=\"0 0 100 80\"><use xlink:href=\"/static/matrix-theme/material-icons.svg#speaker\"></use></svg></div>\n        <div class=\"control\" ng-class=\"{true: 'on'}[itemValue('HABPanel_Dashboard', true)=='Beleuchtung']\" ng-click=\"sendCmd('HABPanel_Dashboard', 'Beleuchtung')\"><svg viewBox=\"0 0 100 80\"><use xlink:href=\"/static/matrix-theme/material-icons.svg#lightbulb-on-outline\"></use></svg></div>\n        <div class=\"control\" ng-class=\"{true: 'on'}[itemValue('HABPanel_Dashboard', true)=='Wetter']\" ng-click=\"sendCmd('HABPanel_Dashboard', 'Wetter')\"><svg viewBox=\"0 0 100 80\"><use xlink:href=\"/static/matrix-theme/material-icons.svg#weather-partlycloudy\"></use></svg></div>\n        <div class=\"control\" ng-class=\"{true: 'on'}[itemValue('HABPanel_Dashboard', true)=='Wohnung']\" ng-click=\"sendCmd('HABPanel_Dashboard', 'Wohnung')\"><svg viewBox=\"0 0 100 80\"><use xlink:href=\"/static/matrix-theme/material-icons.svg#home-outline\"></use></svg></div>\n      </div>\n    </div>\n  </div>\n</div>\n<div class=\"section\">\n  <div class=\"bigDash\">\n    <div class=\"top\">\n      <div class=\"value\">\n        <div class=\"main\">\n          <ds-widget-clock show-digital digital-format=\"'HH'\" style=\"font-weight: bold;\"></ds-widget-clock>\n        </div>\n        <div class=\"main\">\n          <ds-widget-clock show-digital digital-format=\"'mm'\" style=\"font-weight: 100;\"></ds-widget-clock>\n        </div>\n        <div class=\"decimals\">\n          <ds-widget-clock show-digital digital-format=\"'ss'\" style=\"display: inline-block;\"></ds-widget-clock>\n        </div>\n      </div>\n    </div>\n    <div class=\"bottom\">\n      <div class=\"value\">\n      <!-- <div class=\"value\" style=\"text-align:center;float:none\"> -->\n        <ds-widget-clock show-digital digital-format=\"'EEEE, dd. MMMM yyyy'\"></ds-widget-clock>\n      </div>\n    </div>\n    <div class=\"graph\">\n      <object data=\"/static/matrix-theme/shadow.svg?{{itemValue('Sonne_Azimut')}}\" type=\"image/svg+xml\" height=\"150\"></object>      \n    </div>\n  </div>\n  <div class=\"bigDash\">\n    <!--    <div class=\"description\">Innen</div>    -->\n    <div class=\"top\">\n      <div class=\"icon on\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/material-icons.svg#thermometer\"></use></svg></div>\n      <div class=\"value\">\n        <div class=\"main\">{{(itemValue('Thermostat_Temperatur').split(' ')[0] | number:0)}}</div>\n        <div class=\"decimals\">,{{(itemValue('Thermostat_Temperatur').split(' ')[0] | number:1).split(',')[1]}}</div>\n        <div class=\"sub\">&#176C</div>\n      </div>\n    </div>\n    <div class=\"bottom\">\n      <div class=\"icon off\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/material-icons.svg#trending-neutral\"></use></svg></div>\n      <div class=\"value\">{{itemValue('Thermostat_Setpoint')}}</div>\n    </div>\n    <div class=\"graph\">\n      <iframe src=\"https://boemue.nsupdate.info:3000/d-solo/000000003/habpanel?panelId=1&orgId=1&refresh=30s&tab=general&from=now-24h&to=now\" width=\"90%\" height=\"150\" frameborder=\"0\"></iframe>\n      <div class=\"legend\">Letzte 24h</div>\n    </div>\n  </div>\n</div>"
}